{
	"name": "vs-chat",
	"displayName": "vs-chat",
	"description": "messenger extension for developers working on collaborative projects",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.55.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:vs-chat.helloWorld",
		"onCommand:vs-chat.askQuestion",
		"onCommand:vs-chat.chatroom",
		"onCommand:vs-chat.refresh"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vs-chat.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "vs-chat.askQuestion",
				"title": "Ask a Question"
			},
			{
				"command": "vs-chat.chatroom",
				"category": "vs-chat",
				"title": "Chat Room"
			},
			{
				"command": "vs-chat.refresh",
				"category": "vs-chat",
				"title": "Refresh View"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "concurrently \"rollup -c -w\" \"webpack --watch\"",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^18.0.0",
		"@rollup/plugin-node-resolve": "^11.2.1",
		"@rollup/plugin-typescript": "^8.2.1",
		"@tsconfig/svelte": "^1.0.10",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.55.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"rollup": "^2.45.2",
		"rollup-plugin-svelte": "^7.1.0",
		"rollup-plugin-terser": "^7.0.2",
		"svelte": "^3.37.0",
		"svelte-check": "^1.4.0",
		"svelte-preprocess": "^4.7.0",
		"ts-loader": "^8.0.14",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0",
		"webpack": "^5.19.0",
		"webpack-cli": "^4.4.0"
	},
	"dependencies": {
		"axios": "^0.21.1",
		"body-parser": "^1.19.0",
		"express": "^4.17.1",
		"http": "0.0.1-security",
		"mongoose": "^5.12.5",
		"socket.io": "^4.0.1"
	}
}
